<!--<h1>Admin::Genres#edit</h1>-->
<h1><span style="background-color:lightgray">　ジャンル編集　</span></h1>
<br>

<%= form_with model: @edit_admin_genre, url: admin_genre_path do |f| %>
<table border="0" style="display:flex; justify-content:center">
  <tr>
    <td width="120">ジャンル名</td>
    <td width="250"><%= f.text_field :name %></td>
    <td width="150"><%= f.submit '変更を保存', class: "btn btn-success" %></td>
  </tr>
</table>
<% end %>